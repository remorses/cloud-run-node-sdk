        "sucrase": "^3.12.1",
        "typescript": "^3.7.5"
    },
    "dependencies": {
        "@google-cloud/logging": "^7.3.0",
        "@google-cloud/monitoring": "^2.0.0",
        "@types/lodash.flatten": "^4.4.6",
        "@types/uuid": "^8.0.0",
        "await-sleep": "^0.0.1",
